extends layout

block content   
    div(ng-app='myApp', ng-controller='customersCtrl')
      ul
        li(ng-repeat='x in myData')
          | {{ x.type }}
          {{x.ownership_type}}
          {{x.price+" "+x.price_type}}
          {{x.area+ " "+x.area_unit }}
          {{x.full_address.city + " " + x.full_address.zipcode}}
          {{x.type_of_advertisement}}
      
    script(src='/javascripts/listing_angular.js')
     
     